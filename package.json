    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.2.0",
    "prettier": "^2.3.2",
    "typescript": "^3.7.4",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "html-webpack-plugin": "^5.3.2"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --mode=development --hot --inline --port 9002 --history-api-fallback",
    "build": "cross-env NODE_ENV=production webpack",
    "test:unit": "jest",
    "test:coverage": "CI=true yarn run test:unit --env=jsdom --coverage",
    "lint": "eslint --ext js,jsx,ts,tsx src",
    "lint:types": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "fmt": "prettier --write './src/**/*.{ts,tsx,js,jsx}'",
    "fmt:check": "prettier --check './src/**/*.{ts,tsx,ts,sx}'"
  },
  "sideEffects": false
}
